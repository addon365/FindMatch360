@model AccountAdminCreateViewModel 
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Create";
   
}
<!--page-wrapper-->
<!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">AccountAdmin</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"><a asp-controller="AccountAdmin" asp-action="Create"></a>Create</li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-controller="admin" asp-action="CreateProfile">
                <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
            </a>
        </div>
    </div>
</div>
<!--end breadcrumb-->


<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                        <div class="admin-title">
                            <form  asp-action="Create">
                                <div class="form-row">
                                    <div class="form-group col-md-12">
                                        <label>Name</label>
                                        <input asp-for="@Model.Name"   placeholder="Name" class="form-control" type="text">
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label>Email Id</label>
                                        <input type="email" asp-for="LoginEmailId" class="form-control"  placeholder="Email">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <button type="submit" value="Create"  class="btn btn-danger">Submit</button>
                                </div>
                            </form>
                    </div>
                </div>
            </div>
        </div>
    </div>


