@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">AccountAdmin</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                    <a asp-controller="AccountAdmin" asp-action="Details">All</a>
                </li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-controller="admin" asp-action="CreateProfile">
                <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
            </a>
        </div>
    </div>

</div>
<!--end breadcrumb-->
<!--end breadcrumb-->
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                <div class="col-md-12">
                  
                        <form asp-action="Index" method="get">
                            <div class="form-actions no-color">
                                <p>
                                    Find by name: <input type="text" name="SearchString" value="@ViewData["CurrentFilter"]" />
                                    <input type="submit" value="Search" class="btn btn-default" /> |
                                    <a asp-action="Index">Back to Full List</a>
                                </p>
                            </div>
                        </form>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>

                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr>
                                    <td>
                                    </td>
                                    <td>
                                        <a asp-action="Edit" asp-route-id="#">Edit</a> |
                                        <a asp-action="Details" asp-route-id="#">Details</a> |
                                        <a asp-action="Delete" asp-route-id="#">Delete</a>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
            </div>
        </div>
    </div>
</div>



