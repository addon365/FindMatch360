    <style>
        .search_dropdown_box {
            width: 100%;
            position: absolute;
            z-index: 999999999;
            display: none;
            padding: 0;
            margin-top: 30px;
        }

        .search_box_ul li {
            cursor: pointer;
            font: 14px Montserrat-Regular;
        }

        .search_dropdown_box li {
            width: 100%;
            list-style: none;
            background: #fff;
            padding: 10px;
            z-index: 999999999;
        }

        .searchbar_img {
            margin: 0 10px;
            width: 40px;
            height: 40px;
            border-radius: 50%;
        }

        .filder-but, .filder-but:hover, .filder-but:active, .filder-but:focus {
            color: #fff;
            background: #00b903;
            display: inline-block;
            padding: 6px 15px;
            margin: 9px 0;
            cursor: pointer;
            border-radius: 0;
            text-decoration: none;
        }

            .filder-but i {
                padding-right: 6px;
            }

        .filder-container {
            position: absolute;
            left: 0;
            right: 0;
            height: 0;
            overflow: hidden;
            background: #fff;
            border-bottom: 6px solid #e11022;
            z-index: 3;
        }

            .filder-container.open {
                height: auto;
                padding: 20px;
            }
    </style>


    <style>

.custa label{
	position: relative;
	cursor: pointer;
	font-size: 14px;
    font-weight: 400;
}

.custa input[type="checkbox"]{
	position: absolute;
	right: 9000px;
}

/*Check box*/
.custa input[type="checkbox"] + .label-text:before{
	content: "\f096";
	font-family: "FontAwesome";
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing:antialiased;
	width: 1em;
	display: inline-block;
	margin-right: 5px;
}

.custa input[type="checkbox"]:checked + .label-text:before{
	content: "\f14a";
	color: #2980b9;
	animation: effect 250ms ease-in;
}

.custa input[type="checkbox"]:disabled + .label-text{
	color: #aaa;
}

.custa input[type="checkbox"]:disabled + .label-text:before{
	content: "\f0c8";
	color: #ccc;
}
 </style>
   
