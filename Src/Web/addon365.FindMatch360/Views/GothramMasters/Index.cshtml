@model IEnumerable<addon365.FindMatch360.Models.Masters.GothramMaster>

@{
    ViewData["Title"] = "Index";
}
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!--page-wrapper-->
<!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">Gothram</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"> All</li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-controller="GothramMasters" asp-action="Index">
                <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
            </a>
        </div>
    </div>

</div>
<!--end breadcrumb-->


<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                <h1>Index</h1>

                <p>
                    <a asp-action="Create">Create New</a>
                </p>
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.GothramName)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.GothramName)
                                </td>
                                <td>
                                    <a asp-action="Edit" asp-route-id="@item.GothramMasterId">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.GothramMasterId">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.GothramMasterId">Delete</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
