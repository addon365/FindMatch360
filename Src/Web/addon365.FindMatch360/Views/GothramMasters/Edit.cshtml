@model addon365.FindMatch360.Models.Masters.GothramMaster

@{
    ViewData["Title"] = "Edit";
}

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!--page-wrapper-->
<!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">Gothram</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"> Edit</li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-controller="GothramMasters" asp-action="Index">
                <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
            </a>
        </div>
    </div>

</div>
<!--end breadcrumb-->

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                <div class="col-md-12">
                    <h1>Edit</h1>

                    <h4>GothramMaster</h4>
                    <hr />
                    <div class="row">
                        <div class="col-md-4">
                            <form asp-action="Edit">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="GothramMasterId" />
                                <div class="form-group">
                                    <label asp-for="GothramName" class="control-label"></label>
                                    <input asp-for="GothramName" class="form-control" />
                                    <span asp-validation-for="GothramName" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <input type="submit" value="Save" class="btn btn-primary" />
                                </div>
                            </form>
                        </div>
                    </div>

                    <div>
                        <a asp-action="Index">Back to List</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

                    @section Scripts {
                        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
                    }
