@model IEnumerable<addon365.FindMatch360.Models.Masters.SubCasteMaster>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Index";
}


<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">Subcaste</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"><a asp-action="Create">All </a></li>
            </ol>
        </nav>
    </div>
</div>


<div class="user-profile-page">
    <div class="card radius-15">
        <div class="card-body">
            <div class="row">
                <div class="col-12 col-lg-12 col-md-12  col-sm-6">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.SubCasteName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ParentCaste)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SubCasteName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ParentCaste.CasteMasterId)
                                    </td>
                                    <td>
                                        <a asp-action="Edit" asp-route-id="@item.SubCasteMasterId">Edit</a> |
                                        <a asp-action="Details" asp-route-id="@item.SubCasteMasterId">Details</a> |
                                        <a asp-action="Delete" asp-route-id="@item.SubCasteMasterId">Delete</a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
  
            
