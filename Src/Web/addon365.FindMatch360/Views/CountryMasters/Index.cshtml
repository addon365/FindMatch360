@model IEnumerable<addon365.FindMatch360.Models.Masters.CountryMaster>

@{
    ViewData["Title"] = "Index";
}


@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">Dashboard</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"><a asp-action="Create">Create Profile </a></li>
            </ol>
        </nav>
    </div>

</div>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                <div class="col-md-12">

                    <table class="table">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.CountryName)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CountryName)
                                    </td>
                                    <td>
                                        <a asp-action="Edit" asp-route-id="@item.CountryMasterId">Edit</a> |
                                        <a asp-action="Details" asp-route-id="@item.CountryMasterId">Details</a> |
                                        <a asp-action="Delete" asp-route-id="@item.CountryMasterId">Delete</a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
         
