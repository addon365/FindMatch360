@model addon365.FindMatch360.Models.Masters.CasteMaster

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Details";
}
<!--page-wrapper-->
<div class="page-wrapper">
    <!--page-content-wrapper-->
    <div class="page-content-wrapper">
        <div class="page-content">
            <!--breadcrumb-->
            <div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
                <div class="breadcrumb-title pr-3">Dashboard</div>
                <div class="pl-3">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb mb-0 p-0">
                            <li class="breadcrumb-item">
                                <a href="javascript:;"><i class='bx bx-home-alt'></i> </a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page"><a asp-action="Index">Create caste </a></li>
                        </ol>
                    </nav>
                </div>
                <div class="ml-auto">
                    <div class="btn-group">
                        <a asp-controller="admin" asp-action="CreateProfile">
                            <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
                        </a>
                    </div>
                </div>

            </div>
            <!--end breadcrumb-->
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.CasteName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.CasteName)
                </dd>
            </dl>
            <div>
                <a asp-action="Edit" asp-route-id="@Model.CasteMasterId">Edit</a> |

            </div>

        </div>
    </div>
    <!--end page-content-wrapper-->
</div>
<!--end page-wrapper-->

