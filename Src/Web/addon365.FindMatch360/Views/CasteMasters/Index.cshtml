@model IEnumerable<addon365.FindMatch360.Models.Masters.CasteMaster>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Index";
}




<section class="main-content">

    <div class="row">
        <div class="col-md-12 card">
                <h1>Index</h1>
                <p>
                    <a asp-action="Create">Create New</a>
                </p>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.CasteName)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CasteName)
                                </td>
                                <td>
                                    <a asp-action="Edit" asp-route-id="@item.CasteMasterId">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.CasteMasterId">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.CasteMasterId">Delete</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                <div class="card-footer">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Type your message here...">
                        <span class="input-group-btn">
                            <button class="btn btn-primary"><i class="fa fa-send"></i></button>
                        </span>
                    </div>
                </div>
        </div>
    </div>
</section>


