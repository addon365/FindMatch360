@model AdminAllMemberViewModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    int SNo = (Model.TotalMembers.PageStartRecordNumber + 1);
}


            <!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">Profile</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">All</li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-action="Index">
                <button type="button" class="btn btn-primary"> Back to List</button>
            </a>
        </div>
    </div>
</div>

            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="card-title">
                                <h4 class="mb-0">All Member</h4>
                            </div>
                            <hr>
                            <div class="table-responsive">
                                <div id="example2_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row"><div class="col-sm-12 col-md-6"><div class="dt-buttons btn-group">         
                                        <button class="btn btn-secondary buttons-copy buttons-html5" tabindex="0" aria-controls="example2" type="button"><span>Copy</span></button> <button class="btn btn-secondary buttons-excel buttons-html5" tabindex="0" aria-controls="example2" type="button"><span>Excel</span></button> <button class="btn btn-secondary buttons-pdf buttons-html5" tabindex="0" aria-controls="example2" type="button"><span>PDF</span></button> <button class="btn btn-secondary buttons-print" tabindex="0" aria-controls="example2" type="button"><span>Print</span></button> <button class="btn btn-secondary buttons-collection dropdown-toggle buttons-colvis" tabindex="0" aria-controls="example2" type="button" aria-haspopup="true"><span>Column visibility</span></button> </div></div><div class="col-sm-12 col-md-6"><div id="example2_filter" class="dataTables_filter"><label>Search:<input type="search" class="form-control form-control-sm" placeholder="" aria-controls="example2"></label></div></div></div><div class="row">
                                        <div class="col-sm-12">
                                            <table id="example2" class="table table-striped table-bordered dataTable" style="width: 100%;" role="grid" aria-describedby="example2_info">
                                                <thead>
                                                    <tr role="row">
                                                        <th class="sorting_asc" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 20px;">S.No</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 30px;">Reg.No</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending" style="width: 72px;">Reg.Date</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Age: activate to sort column ascending" style="width: 160px;">Name</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Start date: activate to sort column ascending" style="width: 62px;">Caste</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 62px;">Job</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 62px;">Qualification</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 62px;">Place</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 62px;">Salary</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 62px;">Edit</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                 
                                                    @foreach (var item in Model.TotalMembers)
                                                    {
                                                    <tr role="row" class="odd">
                                                        <td class="sorting_1">@SNo</td>
                                                        <td> Reg-@SNo</td>
                                                        <td>@Html.DisplayFor(modelItem => item.RegDate)</td>
                                                        <td> @Html.DisplayFor(modelItem => item.ProfileName)</td>
                                                        <td> @Html.DisplayFor(modelItem => item.Caste)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.Job)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.Qualification)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.Place)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.MonthlyRevenue)</td>
                                                        <td>
                                                            <a asp-action="EditProfile" asp-route-id="@item.ProfileMasterId">Edit</a> @*|
            <a asp-action="Details" asp-route-id="@item.MatrimonyProfileId">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.MatrimonyProfileId">Delete</a>*@
                                                        </td>
                                                    </tr>
                                                    SNo=SNo+1;
                                                    }
                                                    


                                                </tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th rowspan="1" colspan="1">S.No</th>
                                                        <th rowspan="1" colspan="1">Reg.No</th>
                                                        <th rowspan="1" colspan="1">Reg.Date</th>
                                                        <th rowspan="1" colspan="1">Name</th>
                                                        <th rowspan="1" colspan="1">Caste</th>
                                                        <th rowspan="1" colspan="1">Job</th>
                                                        <th rowspan="1" colspan="1">Qualification</th>
                                                        <th rowspan="1" colspan="1">Place</th>
                                                        <th rowspan="1" colspan="1">Monthly Revenue</th>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="row">
                                          @{
                                              var prevDisabled = !Model.TotalMembers.HasPreviousPage ? "disabled" : "";
                                              var nextDisabled = !Model.TotalMembers.HasNextPage ? "disabled" : "";
                        }
                                        <div class="col-sm-12 col-md-5">
                                            <div class="dataTables_info" id="example2_info" role="status" aria-live="polite">Showing @(Model.TotalMembers.PageStartRecordNumber+1) to @Model.TotalMembers.PageEndRecordNumber of @Model.TotalMembers.TotalRecords entries</div>
                                        </div>
                                       <div class="col-sm-12 col-md-7">
                                    <div class="dataTables_paginate paging_simple_numbers" id="example2_paginate">
                                           
                                        <ul class="pagination">
                                            <li class="paginate_button page-item previous disabled" id="example2_previous">
                                                <a asp-action="AllMembers"
                                                   asp-route-sortOrder="@ViewData["CurrentSort"]"
                                                   asp-route-pageNumber="@(Model.TotalMembers.PageIndex - 1)"
                                                   asp-route-currentFilter="@ViewData["CurrentFilter"]"
                                                   class="btn btn-default @prevDisabled">
                                                    Previous
                                                </a>
                                            </li>

                                            @*<li class="paginate_button page-item active">
               <a href="#" aria-controls="example2" data-dt-idx="1" tabindex="0" class="page-link">1</a>
        </li>*@
                                            @for (int i = 1; i <= Model.TotalMembers.TotalPages; i++)
                                            {
                                                
                                                    var IsActivePage = Model.TotalMembers.PageIndex==i ? "active" : "";
                                                
                                                    <li class="paginate_button page-item @IsActivePage">
                                                        <a asp-action="AllMembers"
                                                       asp-route-sortOrder="@ViewData["CurrentSort"]"
                                                       asp-route-pageNumber="@i"
                                                       asp-route-currentFilter="@ViewData["CurrentFilter"]"
                                                        aria-controls="example2" data-dt-idx="2" tabindex="0" class="page-link">@i</a>
                                                    </li>
                                            }
                                                @*
            <li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="3" tabindex="0" class="page-link">3</a></li>
            <li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="4" tabindex="0" class="page-link">4</a></li>
            <li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="5" tabindex="0" class="page-link">5</a></li>
            <li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="6" tabindex="0" class="page-link">6</a></li>*@
                                                <li class="paginate_button page-item next" id="example2_next">
                                                    <a asp-action="AllMembers"
                                                       asp-route-sortOrder="@ViewData["CurrentSort"]"
                                                       asp-route-pageNumber="@(Model.TotalMembers.PageIndex + 1)"
                                                       asp-route-currentFilter="@ViewData["CurrentFilter"]"
                                                       class="btn btn-default @nextDisabled">
                                                        Next
                                                    </a>
                                                </li>
                                            </ul></div></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    <!--end page-content-wrapper-->

<!--end page-wrapper-->
<!--page-wrapper-->



