@model AdminDashBoardViewModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}


<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- 						Content Start	 						-->
<!-- ============================================================== -->
<!--page-wrapper-->
<div class="page-wrapper">
    <!--page-content-wrapper-->
    <div class="page-content-wrapper">
        <div class="page-content">
            <div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
                <div class="breadcrumb-title pr-3">Dashboard</div>
                <div class="pl-3">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb mb-0 p-0">
                            <li class="breadcrumb-item">
                                <a href="javascript:;"><i class='bx bx-home-alt'></i></a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page">Profile</li>
                        </ol>
                    </nav>
                </div>
                <div class="ml-auto">
                    <div class="btn-group">
                        <a asp-controller="admin" asp-action="CreateProfile">
                            <button type="button" class="btn btn-primary">Create New</button>
                        </a>
                     
                     
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-12 col-lg-3">
                    <div class="card radius-15 bg-primary">
                        <div class="card-body">
                            <div class="media align-items-center">
                                <div class="media-body">
                                    <h5 class="mb-0 font-weight-bold text-white">Total Member</h5>
                                    <h4 class="mb-0 font-weight-bold text-white">@Model.TotalMembers</h4>
                                </div>
                                <div class="font-35 text-white">
                                    <i class="bx bx-cart-alt"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-lg-3">
                    <div class="card radius-15 bg-warning">
                        <div class="card-body">
                            <div class="media align-items-center">
                                <div class="media-body">
                                    <h5 class="mb-0 font-weight-bold text-dark">Paid Member</h5>
                                    <h4 class="mb-0 font-weight-bold text-dark">@Model.PaidMembers</h4>

                                </div>
                                <div class="font-35 text-dark">
                                    <i class="bx bx-wallet"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-lg-3">
                    <div class="card radius-15 bg-danger">
                        <div class="card-body">
                            <div class="media align-items-center">
                                <div class="media-body">
                                    <h5 class="mb-0 font-weight-bold text-white">Inactive</h5>
                                    <h4 class="mb-0 font-weight-bold text-dark">....</h4>

                                </div>
                                <div class="font-35 text-white">
                                    <i class="bx bx-cloud-download"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-lg-3">
                    <div class="card radius-15 bg-success">
                        <div class="card-body">
                            <div class="media align-items-center">
                                <div class="media-body">
                                    <h5 class="mb-0 font-weight-bold text-white">Visitors</h5>
                                    <h4 class="mb-0 font-weight-bold text-white">@Model.Visitors</h4>

                                </div>
                                <div class="font-35 text-white">
                                    <i class="bx bx-group"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="card-title">
                                <h4 class="mb-0">DataTable Import</h4>
                            </div>
                            <hr>
                            <div class="table-responsive">
                                <div id="example2_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row">
                                        <div class="col-sm-12 col-md-6">
                                            <div class="dt-buttons btn-group">
                                                <button class="btn btn-secondary buttons-copy buttons-html5" tabindex="0" aria-controls="example2" type="button">
                                                    <span>Copy</span>
                                                </button>
                                                <button class="btn btn-secondary buttons-excel buttons-html5" tabindex="0" aria-controls="example2" type="button">
                                                    <span>Excel</span>
                                                </button>

                                                <a>
                                                    <button class="btn btn-secondary buttons-pdf buttons-html5" tabindex="0" aria-controls="example2" type="button"  onclick="window.open('file.doc')"><span>PDF</span></button>
                                                </a>
                                                <button class="btn btn-secondary buttons-print" tabindex="0" aria-controls="example2" type="button" onclick="window.print()"><span>Print</span></button>
                                                <button class="btn btn-secondary buttons-collection dropdown-toggle buttons-colvis" tabindex="0" aria-controls="example2" type="button" aria-haspopup="true"><span>Column visibility</span></button>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div id="example2_filter" class="dataTables_filter">
                                                <label>Search:<input type="search" class="form-control form-control-sm" placeholder="" aria-controls="example2"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table id="example2" class="table table-striped table-bordered dataTable" style="width: 100%;" role="grid" aria-describedby="example2_info">
                                                <thead>
                                                    <tr role="row">
                                                        <th class="sorting_asc" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 103px;">
                                                            S.No
                                                        </th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 163px;">Reg.No</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending" style="width: 72px;">Reg.Date</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Age: activate to sort column ascending" style="width: 25px;">Name</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Start date: activate to sort column ascending" style="width: 62px;">Caste</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 52px;">Job</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 52px;">Qualification</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 52px;">Place</th>
                                                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending" style="width: 52px;">Salary</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model.RecentMembers)
                                                    {
                                                        <tr role="row" class="odd">
                                                            <td class="sorting_1">@Html.DisplayFor(modelItem => item.SNo)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.RegNo)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.RegDate)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Name)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Caste)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Job)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Qualification)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Place)</td>
                                                            <td>@Html.DisplayFor(modelItem => item.Salary)</td>
                                                        </tr>
                                                    }

                                                </tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th rowspan="1" colspan="1">S.No</th>
                                                        <th rowspan="1" colspan="1">Reg.No</th>
                                                        <th rowspan="1" colspan="1">Reg.Date</th>
                                                        <th rowspan="1" colspan="1">Name</th>
                                                        <th rowspan="1" colspan="1">Caste</th>
                                                        <th rowspan="1" colspan="1">Job</th>
                                                        <th rowspan="1" colspan="1">Qualification</th>
                                                        <th rowspan="1" colspan="1">Place</th>
                                                        <th rowspan="1" colspan="1">Salary</th>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                    </div><div class="row"><div class="col-sm-12 col-md-5"><div class="dataTables_info" id="example2_info" role="status" aria-live="polite">Showing 1 to 10 of 57 entries</div></div><div class="col-sm-12 col-md-7"><div class="dataTables_paginate paging_simple_numbers" id="example2_paginate"><ul class="pagination"><li class="paginate_button page-item previous disabled" id="example2_previous"><a href="#" aria-controls="example2" data-dt-idx="0" tabindex="0" class="page-link">Prev</a></li><li class="paginate_button page-item active"><a href="#" aria-controls="example2" data-dt-idx="1" tabindex="0" class="page-link">1</a></li><li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="2" tabindex="0" class="page-link">2</a></li><li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="3" tabindex="0" class="page-link">3</a></li><li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="4" tabindex="0" class="page-link">4</a></li><li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="5" tabindex="0" class="page-link">5</a></li><li class="paginate_button page-item "><a href="#" aria-controls="example2" data-dt-idx="6" tabindex="0" class="page-link">6</a></li><li class="paginate_button page-item next" id="example2_next"><a href="#" aria-controls="example2" data-dt-idx="7" tabindex="0" class="page-link">Next</a></li></ul></div></div></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--end row-->





<script>
    function download() {
        var iframe = document.getElementById('invisible');
        iframe.src = "file.doc";
    }
</script>

