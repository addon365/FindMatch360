@model addon365.FindMatch360.Models.Masters.CityMaster

@{
    ViewData["Title"] = "Edit";
}

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!--page-wrapper-->
<div class="page-wrapper">
    <!--page-content-wrapper-->
    <div class="page-content-wrapper">
        <div class="page-content">
            <!--breadcrumb-->
            <div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
                <div class="breadcrumb-title pr-3">Dashboard</div>
                <div class="pl-3">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb mb-0 p-0">
                            <li class="breadcrumb-item">
                                <a href="javascript:;"><i class='bx bx-home-alt'></i> </a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page"><a asp-action="Index">Create caste </a></li>
                        </ol>
                    </nav>
                </div>
                <div class="ml-auto">
                    <div class="btn-group">
                        <a asp-controller="admin" asp-action="CreateProfile">
                            <button type="button" class="btn btn-primary" asp-action="Index"> Back to List</button>
                        </a>
                    </div>
                </div>

            </div>
            <form asp-action="Edit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="CityMasterId" />
                <div class="form-group">
                    <label asp-for="CityName" class="control-label"></label>
                    <input asp-for="CityName" class="form-control" />
                    <span asp-validation-for="CityName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="StateMasterId" class="control-label"></label>
                    <select asp-for="StateMasterId" class="form-control" asp-items="ViewBag.StateMasterId"></select>
                    <span asp-validation-for="StateMasterId" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </form>

        </div>
    </div>
    <!--end page-content-wrapper-->
</div>
<!--end page-wrapper-->

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
