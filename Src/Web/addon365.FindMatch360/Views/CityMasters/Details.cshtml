@model addon365.FindMatch360.Models.Masters.CityMaster

@{
    ViewData["Title"] = "Details";
}
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!--breadcrumb-->
<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3">City</div>
    <div class="pl-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="javascript:;"><i class='bx bx-home-alt'></i> </a>
                </li>
                <li class="breadcrumb-item active" aria-current="page"><a asp-action="Index">Details</a></li>
            </ol>
        </nav>
    </div>
    <div class="ml-auto">
        <div class="btn-group">
            <a asp-action="Index">
                <button type="button" class="btn btn-primary"> Back to List</button>
            </a>
        </div>
    </div>
</div>
<!--end breadcrumb-->


<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <!--end breadcrumb-->
                <div class="col-md-12">
                    <dl class="row">

                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.CityName)
                        </dt>
                        <dd class="col-sm-6">
                            @Html.DisplayFor(model => model.CityName)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.State)
                        </dt>
                        <dd class="col-sm-6">
                            @Html.DisplayFor(model => model.State.StateMasterId)
                        </dd>
                    </dl>
                    <div>
                        <a asp-action="Edit" asp-route-id="@Model.CityMasterId">Edit</a> |
                        <a asp-action="Index">Back to List</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
          
