@page
@model addon365.FindMatch360.Views.Account.campaignregistrationaboutModel


@{
    Layout = "~/Views/Shared/_CampaignLayout.cshtml";
}
<div class="campaignregistor-form-bg">
    <div class="container">
        <div class="row" id="campaignregistor-wrapper">
            <div class="col-md-5" id="campignregistration-left">
                <div class="campignregistration-title">
                    <img src="~/images/reg-5.png" class="img-fluid" id="img-living">
                    <div class="campignregistration-title-bg">
                        <h5> Someone to love and laugh with</h5>
                    </div>
                </div>
            </div>

            <div class="col-md-7" id="campignregistration-right">
                <div class="signup-form">
                    <form method="post">
                        <h5 class="float-right">Great! You have completed <span class="precentage">90%</span></h5>
                        <h2>Create Account</h2>
                        <p class="lead">
                            Let the world know about your awesomeness
                        </p>
                        <div class="form-group">
                            <label for="comment">Comment:</label>
                            <textarea class="form-control" rows="5" id="comment" name="text"></textarea>
                        </div>
                        <div class="form-group">
                            <a asp-controller="Account" asp-action="campaignregistrationpersonaldetails">
                                <button type="submit" class="btn btn-primary btn-block btn-lg Continue-bg">Continue</button>
                            </a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
